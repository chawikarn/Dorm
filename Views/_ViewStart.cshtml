@{
    if (User.Identity.Name=="admin@mail.com")
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
    else if(User.Identity.IsAuthenticated)
    {
        Layout = "~/Views/Shared/_LayoutUser.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_LayoutUserNotRegisted.cshtml";
    }
}